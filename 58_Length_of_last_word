class Solution:
    def lengthOfLastWord(self, s: str) -> int:
        s_list = s.split(' ')
        n_list =[]
        for word in s_list:
            n_list.append(word)
            if (word ==''):
                n_list.pop()
                
        leng = len(n_list[-1])
        return(leng)
        
